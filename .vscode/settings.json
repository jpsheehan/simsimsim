{
    "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix",
    "files.associations": {
        "sdl_image.h": "c"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "images/*": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "images/*": true
    },
    "explorer.excludeGitIgnore": true
}